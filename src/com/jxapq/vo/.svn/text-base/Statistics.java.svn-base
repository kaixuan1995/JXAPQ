package com.jxapq.vo;

/**
 * @author Administrator
 *
 */
public class Statistics {
	private String Name;
	private int sum;
	
	
	public Statistics() {
		super();
	}
	
	public Statistics(String name, int sum) {
		super();
		Name = name;
		this.sum = sum;
	}
	public String getName() {
		return Name;
	}
	public void setName(String name) {
		Name = name;
	}
	public int getSum() {
		return sum;
	}
	public void setSum(int sum) {
		this.sum = sum;
	}
	
	
}
